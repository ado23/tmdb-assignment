{
  "name": "tmdb-assignment",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "TMDB Assignment",
  "author": "Admir Ćejvanović",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "lint": "eslint ./src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier ./src --write",
    "build": "tsc && eslint ./src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0 && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "generate-api": "npx swagger-typescript-api -p https://developer.themoviedb.org/openapi/64542913e1f86100738e227f -o ./src/core/api -n generatedApi --axios --extract-request-params --extract-request-body --extract-response-body --extract-response-error",
    "update-sprite": "svg-sprite -s --symbol-dest src --symbol-sprite assets/icons/sprite.svg src/assets/icons/svg/*/*.svg"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.5.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "svg-sprite": "^2.0.4",
    "swagger-typescript-api": "^13.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  }
}
